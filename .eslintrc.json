{

    "env":{
        "es2021":true,
        "node":true
    },
    "extends":"eslint:recommended",
    "overrides":[
    ],
    "parserOptions":{
        "ecmaVersion":"latest",
        "sourceType":"module"
    },
    "rules":{
        "no-undef":"off",
        "no-unused-vars":"warn",
        "no-useless-escape":"off",
        "no-useless-catch":"off",
        "no-mixed-spaces-and-tabs":"off"
    }

}